{
  "name": "vue-env",
  "version": "0.1.0",
  "description": "basic env for vue",
  "main": "index.js",
  "scripts": {
    "start": "npm run dev",
    "dev": "npm run prompt && npm run all",
    "serve": "cross-env BABEL_ENV=default node --max_old_space_size=8192 ./node_modules/webpack-dev-server/bin/webpack-dev-server --config  ./config/webpack.config.dev.js --hot",
    "dist": "npm run remove:dist && npm run lint && node --max_old_space_size=8192 ./node_modules/webpack/bin/webpack --config ./config/webpack.config.dist.js",
    "build": "cross-env UGLIFY_JS=true BABEL_ENV=default npm run dist -- -p && npm run copy",
    "build:dev": "cross-env UGLIFY_JS=false BABEL_ENV=default npm run dist && npm run copy",
    "remove:dist": "rm -rf ./dist",
    "copy": "echo 'end'",
    "lint": "esw ./config/*.config.* ./src/pages/**/*.{js,vue}",
    "lint:fix": "npm run lint -- --fix",
    "lint:watch": "npm run lint -- --fix --watch",
    "lint:style": "stylelint --fix --syntax scss src",
    "mock": "cd ./src/mock && npm run start",
    "prompt": "node ./config/prompt.config.js",
    "all": "npm-run-all --parallel --race --continue-on-error lint:watch serve",
    "lint-staged": "lint-staged",
    "pre-commit:install": "cp .hooks/* .git/hooks/",
    "test": "jest --config ./tests/config/jest.config.js"
  },
  "author": "wya-team",
  "license": "MIT",
  "dependencies": {
    "@babel/polyfill": "^7.0.0",
    "highlight.js": "^9.12.0",
    "js-beautify": "^1.8.1",
    "marked": "^0.5.0",
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "wya-fetch": "^0.3.1",
    "wya-sass": "^0.4.0",
    "wya-utils": "^0.1.19"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-proposal-export-default-from": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-bind": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-jsx": "^7.0.0",
    "@babel/plugin-transform-block-scoping": "^7.1.5",
    "@babel/preset-env": "^7.1.6",
    "assets-webpack-plugin": "^3.5.1",
    "autoprefixer": "^8.3.0",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^8.2.3",
    "babel-jest": "^23.4.2",
    "babel-loader": "^8.0.4",
    "babel-plugin-import": "^1.11.0",
    "babel-plugin-transform-vue-jsx": "^4.0.1",
    "copy-webpack-plugin": "^4.5.1",
    "cp-cli": "^1.1.0",
    "cross-env": "^5.1.4",
    "css-loader": "^0.28.11",
    "enzyme": "^3.5.0",
    "enzyme-to-json": "^3.3.4",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-babel": "^5.0.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-markdown": "^1.0.0-beta.6",
    "eslint-plugin-vue": "^4.7.1",
    "eslint-watch": "^3.1.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "fs-extra": "^5.0.0",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "inquirer": "^5.2.0",
    "jest": "^23.5.0",
    "json-server": "^0.14.0",
    "lint-staged": "^7.0.4",
    "mockjs": "^1.0.1-beta3",
    "node-sass": "^4.8.3",
    "npm-run-all": "^4.1.2",
    "open-browser-webpack-plugin": "0.0.5",
    "postcss-flexbugs-fixes": "3.3.0",
    "postcss-loader": "^2.1.4",
    "postcss-smart-import": "^0.7.6",
    "pre-commit": "^1.2.2",
    "precss": "^3.1.2",
    "prompt": "^1.0.0",
    "rimraf": "^2.6.1",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "stylelint": "^9.2.0",
    "stylelint-config-standard": "^18.2.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "url-loader": "^1.0.1",
    "vue-loader": "^15.3.0",
    "vue-style-loader": "^4.1.1",
    "vue-template-compiler": "^2.5.17",
    "webpack": "^4.6.0",
    "webpack-bundle-analyzer": "^2.9.1",
    "webpack-cli": "^3.1.2",
    "webpack-dev-server": "^3.1.3",
    "webpack-merge": "^4.1.2"
  },
  "lint-staged": {
    "{src,config}/**/*.{js,jsx,vue}": [
      "esw --fix",
      "git add"
    ],
    "*.md": [
      "esw --ext '.md' --fix",
      "git add"
    ],
    "*.scss": [
      "stylelint --fix --syntax scss",
      "git add"
    ]
  },
  "pre-commit": [
    "lint-staged"
  ]
}
